// Maps

const map = new Map()
map.set('IN', "India")
map.set('USA', "United States of America")
map.set('Fr', "France")
map.set('IN', "India")    //duplicate nhi lega

console.log(map);

for (const key of map) {
    console.log(key);         //array typeka print ho jata h aise
    
}

for (const [key,value] of map ){        //destructuring kr di
    console.log(key, " :-",value);   //isi tarah object ko krate print to na hota , not iterable like this
    
}

//for in loop

const myObject = {
    js: 'javascript',
    cpp: 'C++',
    rb: "ruby",
    swift: "swift by apple"
}

for (const key in myObject) {          //ha to is trh hoga object iterable 
    //console.log(`${key} shortcut is for ${myObject[key]}`);
}
//for in loop , map me na lga dena