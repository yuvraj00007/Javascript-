const myNums = [1, 2, 3]

const myTotal = myNums.reduce(function (acc, currval) {         //acc ko har bar current vale me add kra jayega 
    console.log(`acc: ${acc} and currval: ${currval}`);         
    return acc + currval
}, 0)               //ye jo zero hai ye accumulator ko di gyi h i.e., initial value

const myTotal2 = myNums.reduce( (acc, curr) => acc+curr, 0)

console.log(myTotal);


const shoppingCart = [
    {
        itemName: "js course",
        price: 2999
    },
    {
        itemName: "py course",
        price: 999
    },
    {
        itemName: "mobile dev course",
        price: 5999
    },
    {
        itemName: "data science course",
        price: 12999
    },
]

const priceToPay = shoppingCart.reduce((acc, item) => acc + item.price, 0)

console.log(priceToPay);