for (let i = 1; i <= 10; i++) {
    //console.log(`Outer loop value: ${i}`);
   for (let j = 1; j <= 10; j++) {
    //console.log(`Inner loop value ${j} and inner loop ${i}`);
    console.log(i + '*' + j + ' = ' + i*j );    //focus on likhne ka tareeka , mtln aise bhi kr skte h
   }
    
}

//for of loop

// for (const element of object) {
    
// }           //syntax
arr=[1,2,3,4,5,4]
for (const num of arr) {
    console.log(num);
    
}